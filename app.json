{
  "expo": {
    "name": "Civilraw",
    "slug": "raghunewapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/civilrawappicon.png",
    "scheme": "civilraw",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "assets/civilrawappicon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.interior.reviewapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/civil-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "assets/civilrawappicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "948cb1e6-c574-4526-857a-50a4ba1780e8"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/948cb1e6-c574-4526-857a-50a4ba1780e8"
    }
  }
}
